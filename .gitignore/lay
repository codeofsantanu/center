    <script>
        function openchangepswddiv() {
            debugger
            var divv = document.getElementById('Changepasswordlanding');
            divv.style.display = "block";
            $("#txtEmail").attr("readonly", true);
            var regId = $('#test1').data('text');
            // Next12
            $("#Next12").kendoButton().data("kendoButton").enable(false);
            $("#ember1033").hide();
            //alert("gbhj");
            $.ajax({
                type: "Get",
                url: "/UserLanding/Landing/GetUserPhotoByUserRegId",
                data: { 'regId': regId },
                datatype: "json",
                cache: false,
                success: function (userdata) {

                    if (userdata != null) {
                        $("#proimg").attr("src", userdata.Uplaod_ImageURL);
                        $("#usname").text(userdata.FullName);
                        $("#txtEmail").val(userdata.User_EmailId);

                    }

                }

            });
        }

        function Changepasswordlandingclose() {
            var divv = document.getElementById('Changepasswordlanding');
            divv.style.display = "none";
        }
        function btnVerifyClick() {

            var EmailId = $("#txtEmail").val();

            $("#btnResend").kendoButton({
                enable: false,
            });
            var Resend = document.getElementById('btnResend');
            Resend.style.backgroundColor = '#c9cdd1';

            $("#VerificationDiv").show();
            $.ajax({
                type: "POST",
                url: "/UserSignup/Signup/MailSend",
                data: { 'toEmail': EmailId },
                datatype: "json",
                cache: false,
                success: function (result) {
                },
                error: function (result) {
                }
            })
        }

        function btnVerifyOtpClick() {
