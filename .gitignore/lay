        <script>

          

            function newServiceRequestclose() {
                var divv3 = document.getElementById('newServiceRequest');
                divv3.style.display = "none";
            }

            function ServiceRequestclose() {
                var divv3 = document.getElementById('ServiceRequest');
                divv3.style.display = "none";
            }


            function finish_click() {
                var divv3 = document.getElementById('ServiceRequest');
                divv3.style.display = "none";
                var reset = document.getElementById("newServiceRequest");
                reset.style.display = "none";
            }

            function Cancel_click() {
                var divv3 = document.getElementById('newServiceRequest');
                divv3.style.display = "none";

            }

        </script>


        <script>
            function confirm_clickForRequest() {
                debugger;
                var validator = $("#newServiceRequest").kendoValidator().data("kendoValidator");

                if (validator.validate()) {
                    var service = $("#ddllservice").data("kendoDropDownList");
                    var serviceArea = $("#dllservicearea").data("kendoDropDownList");
                    var problem = $("#ddllproblem").data("kendoDropDownList");

                    $.ajax({
                        type: "POST",
                        url: "/ListPage/CreateSupport",
                        data: {
                            Title: $("#txtissuee").val(),
                            Service: service.value(),
                            ServiceArea: serviceArea.value(),
                            Problem: problem.value(),
                            AdditionalNote: $("#txtaddnote").val(),
                            PersonName: $("#txtname").val(),
                            PhoneNumber: $("#txtphn").val(),
                            Email: $("#txtemail1").val(),
                            Country: $("#countryid").val(),


                        },
                        datatype: "json",
                        cache: false,

                        success: function (result) {
                            debugger;
                            $("#newServiceRequest").hide();
                            $("#ServiceRequest").show();
                            //alert(result);
                            //myFunction21();
                            //window.location.reload();
                            document.getElementById('txtaddnotee').innerHTML = $("#txtaddnote").val();
                            document.getElementById('txtissue11').innerHTML = $("#txtissuee").val();
                            document.getElementById('ddlservice1').innerHTML = service.text();
                            document.getElementById('dlservicearea').innerHTML = serviceArea.text();
                            document.getElementById('ddlproblem').innerHTML = problem.text();
                            document.getElementById('txtnamee').innerHTML = $("#txtname").val();
                            document.getElementById('txtphnn').innerHTML = $("#txtphn").val();
                            document.getElementById('txtemaill').innerHTML = $("#txtemail1").val();
                            document.getElementById('ddllcountry').innerHTML = $("#ddlcountry").val();
                            document.getElementById('txtsupportid').innerHTML = result;
                            document.getElementById('txtsid').innerHTML = result;

                            var path = window.location.origin;
                            path = path.replace(/\/$/, "");
                            path = decodeURIComponent(path);
                            totUrl = path + "/ClientSupport/ListPage/index";
                            if (document.location == totUrl) {
                                myFunction21();
                            }

                            @*@Html.Label("txtsupportid", (String)ViewBag.User_Unique_RegistrationCode)*@

                        },
                        error: function (result) {
                            debugger;


                        }
                    })
                }

            }


        </script>

        <script>
            function openserviceRequest() {
                debugger;
                var reset = document.getElementById("newServiceRequest");
                reset.style.display = "block";
                $("#txtissuee").val("");
                $("#ddllservice").data("kendoDropDownList").value(-1);
                $("#dllservicearea").data("kendoDropDownList").value(-1);
                $("#ddllproblem").data("kendoDropDownList").value(-1);
                $("#txtaddnote").val("");
                $("#MobilenoPrefix1").data("kendoDropDownList").value(-1);
                $("#txtphn").val("");
                $("#ddlcountry").val("");
                $("#txtname").prop("disabled", true).addClass("k-state-disabled");
                $("#txtemail1").prop("disabled", true).addClass("k-state-disabled");

                $("#txtphn_validationMessage").hide();
                $("#ddlcountry_validationMessage").hide();
                $("#_validationMessage").hide();
                $("#ddllservice_validationMessage").hide();
                $("#dllservicearea_validationMessage").hide();
                $("#ddllproblem_validationMessage").hide();
                $("#txtissuee_validationMessage").hide();
                $.ajax({
                    type: "POST",
                    url: "/ClientSupport/ListPage/Autopopulate",
                    // data: { 'id': id },
                    contentType: 'application/x-www-form-urlencoded; charset=UTF-8',
                    dataType: "json",
                    success: function (data) {
                        debugger;
                        $("#newServiceRequest").show();
                        $("#txtname").val(data.Data[0].FullName);
                        $("#txtemail1").val(data.Data[0].User_EmailId);
                    },
                    error: function (data) {

                    }
                });

            }
        </script>
        @*Service Request End*@
