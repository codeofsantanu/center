                        template: kendo.template($("#templatedomain").html()),

                    });
                },
                error: function () {
                }
            });

            // To Highlight User Domain in which he/she has logged in

            function onDataBound(e) {
                debugger;
                var tanentCompName1 = $('#tanentCompName').data('text');
                var listview = $("#listViewDomain").data("kendoListView");
                var data = listview.dataItems();
                debugger;
                var count = listview.dataSource.total();
                //Set height of listview when listview count exceeds 5
                if (count > 5) {

                    $("#heightScript").css('overflow', 'auto');
                    $("#heightScript").height(218);
                }
                $.each(data, function (i, Tenent_CompanyName) {
                    debugger;
                    if (data[i].Tenent_CompanyName == tanentCompName1) {
                        var item = data[i].Tenent_CompanyName;
                        var uid = $(".listView11", data.Tenent_CompanyName).data("uid");
                        $("#listViewDomain").find("div[data-uid=" + uid + "]").css('background', '#8fb713');
                    }
                });


                //Click event of items of domain listview

              
                $(".listView11").on("click", function () {
                    debugger;
                    var listView = $("#listViewDomain").data("kendoListView");
                    var idx = $(this).index();
                    var item = listView.dataSource.view()[idx];
                    //alert(item.Tanent_Name);
                    $.ajax({
                        type: "POST",
                        url: "/UserList/GetUrl",
                        data: { 'tanentId': item.TenentId },
                        dataType: "json",
                        success: function (data) {
                           // alert("raha");
                            debugger;
                            if (data.status == "1") {
                                if (data != null) {
                                    // window.location = "http" + "/" + item.Tanent_Name;
                                    //window.location.href = "http://" + "/" + data;
                                    window.location.href = "http://" + "/" + data.url + "?DbName=" + data.db + "&" + "Containername=" + data.container + "&" + "tanentname=" + data.tanentname + "&regId=" + data.regId + "&tanentId=" + data.tanentid;
                                }
                                else {

                                }
                            }
                            else {
                                window.location.href = "/CustomizedLogin/SingleSignOn/LicenseExpired";
                            }
                        }
                    });

                });
            }

            






