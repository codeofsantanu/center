        <script>


            function openchangereqdiv() {
                debugger;
                $("#ember1033").hide();
                openserviceRequest();

            }
            function openLockScreen() {
                debugger;
                document.location = '@Url.Action("GetLockScreen", "ClientDashBoard", new { area = "ClientAdmin" })';
                @*$("#ember1033").hide();
                $.removeCookie("CurrentUrl");
                @*var url = '@Url.Action("Index","LockScreen", new { area = "ClientAdmin" })';*@
                //var currenturl = window.location.href;
                //$.cookie("CurrentUrl", currenturl);
                //window.location.href = '/ClientAdmin/LockScreen/Index/';
            }
            function ServiceRequestlandingclose() {
                var divv3 = document.getElementById('ServiceRequestlanding');
                divv3.style.display = "none";
            }

           // $("#body").on('keydown', null, 'ctrl+l', openLockScreen());
            $(document).ready(function () {
                debugger;
                //log("Ready. Press Ctrl+Shift+F9!");
                var isCtrl;
                var isShift;
                // action on key up
                $(document).keyup(function (e) {
                    if (e.which == 17) {
                        isCtrl = false;
                    }
                    if (e.which == 76 || e.which==108) {
                        isShift = false;
                    }
                });
                // action on key down
                $(document).keydown(function (e) {
                    if (e.which == 17) {
                        isCtrl = true;
                    }
                    if (e.which == 76 || e.which == 108) {
                        isShift = true;
                    }
                    if ( isCtrl && isShift) {
                        openLockScreen();
                    }
                });

            });
            function onSelectCountry_Code(e) {
                debugger;
                var dataItem = this.dataItem(e.item);
                var id = dataItem.Country_Id;
                if (id == "") {
                    $("#ddlcountry").val("");
                    //$("#txtMobileNo").val("");
                    // VerifyDisableMob();
                    $("#txtphn").prop("disabled", true).addClass("k-state-disabled");
                    $("#txtphn_validationMessage").hide();

                }
                else {
                    //If country code value not 00 then
                    $("#txtphn").prop("disabled", false).removeClass("k-state-disabled");
                }


                document.getElementById("countryid").value = dataItem.Country_Id;
                //alert(dataItem.Country_Id);
                $.ajax({
                    type: "POST",
                    url: "/Signup/CountryNameBind",
                    data: { id: id },
                    contentType: 'application/x-www-form-urlencoded; charset=UTF-8',
                    dataType: "json",
                    success: function (data) {
                        $("#ddlcountry").val(data.Conuntry_Name);

                    },
                });
            }

            $('#txtphn').keypress(function (e) {
                var regex = new RegExp("^[0-9]+$");
                var str = String.fromCharCode(!e.charCode ? e.which : e.charCode);
                if (regex.test(str)) {
                    return true;
                }
                e.preventDefault();
                return false;
            });
        </script>



