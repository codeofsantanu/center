        // Log out Code

      function  LogoutUesr() {
            $.ajax({
                type: "POST",
                url: "/UserList/Logout",
                // content: "application/json; charset=utf-8",
                dataType: "json",
                success: function (result) {
                    if (result == 1) {
                        window.location.href = "/CustomizedLogin/SingleSignOn/Index"

                    }
                }
            });
        };






