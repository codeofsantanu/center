    <script>

        function SubmitDisableEmail() {
            $("#btnVerifyMail").kendoButton({
                enable: false,
            });
            var Submit = document.getElementById('btnVerifyMail');
            Submit.style.backgroundColor = '#c9cdd1';

        }
        function SubmitEnableEmail() {
            var y = document.getElementById('btnVerifyMail');
            y.style.backgroundColor = '#8ebc00';
            $("#btnVerifyMail").addClass(".k-primary k-button k-state-enabled").attr("disabled", false);
            document.getElementById("btnVerifyMail").setAttribute("class", ".k-primary k-button k-state-enabled");
        }
        function ResendDisableEmail() {
            $("#btnResendEmail").kendoButton({
                enable: false,
            });
            var Resend = document.getElementById('btnResendEmail');
            Resend.style.backgroundColor = '#c9cdd1';
        }
        function ResendEnableEmail() {
            var z = document.getElementById('btnResendEmail');
            z.style.backgroundColor = '#8ebc00';
            $("#btnResendEmail").addClass(".k-primary k-button k-state-enabled").attr("disabled", false);
            document.getElementById("btnResendEmail").setAttribute("class", ".k-primary k-button k-state-enabled");
        }
        function VerifyDisableEmail() {
            $("#btnMailSendOTP").kendoButton({
                enable: false,
            });
            var MailVerify = document.getElementById('btnMailSendOTP');
            MailVerify.style.backgroundColor = '#c9cdd1';
        }
        function VerifyEnableEmail() {
            var Verify = document.getElementById('btnMailSendOTP');
            Verify.style.backgroundColor = '#8ebc00';
            $("#btnMailSendOTP").addClass(".k-primary k-button k-state-enabled").attr("disabled", false);
            document.getElementById("btnMailSendOTP").setAttribute("class", ".k-primary k-button k-state-enabled");
        }


        function DisableEnableResendEmail() {


            if ($("#VerificationDiv").is(":hidden")) {
                $("#OTPBlank").hide();
                $("#OTPInvalid").hide();
                $("#OTPExpire").hide();
            } else if ($("#VerificationDiv").is(":visible")) {
                $("#OTPInvalid").hide();
                $("#OTPBlank").hide();
                $("#OTPExpire").show();
            }
            //Enable Resend
            ResendEnableEmail();
            //Disable Submit
            SubmitDisableEmail();


        }

        function DisableEnableSubmitResendEmail() {

            if ($("#VerificationDiv").is(":hidden")) {
                $("#OTPBlank").hide();
                $("#OTPInvalid").hide();
                $("#OTPExpire").hide();
            } else if ($("#VerificationDiv").is(":visible")) {
                $("#OTPInvalid").hide();
                $("#OTPBlank").hide();
                $("#OTPExpire").show();
            }


            //Disable submit
            SubmitDisableEmail();

            //Enable resend
            ResendEnableEmail();

            // return false;

        }

        function EmailsubmitResendActiveOperation() {

            var EmailOTP = document.getElementById("txtVerifyEmailOTP");

            //OTP Paste Event in txtVerifyEmailOTP
            $("#txtVerifyEmailOTP").bind("paste", function (e) {
                var pastedData = e.originalEvent.clipboardData.getData('text');
                var len = pastedData.length;

            });

            //OTP Click Event in txtVerifyEmailOTP
            EmailOTP.onkeypress = function () {    //Onkey press event on otptext box
                debugger;
            }

            //Set Timeout Event in txtVerifyEmailOTP
            setTimeout(DisableEnableResendEmail, 120000);
        }

        //-----OTP Sending functionality for Email and disable mail-send btn



        //-----OTP verification functionality for Email
        function btnVerifyMail_Click() {

            var EmailId = $("#txtEmail").val();
            var OTP = $("#txtVerifyEmailOTP").val();
            if (OTP == "") {
                $("#OTPBlank").show();
                return false;
            }
            $.ajax({
                type: "POST",
                url: "/UserSignup/Signup/OTPVerificationEmail",
                data: { 'OTP': OTP, 'EmailId': EmailId },
                datatype: "json",
                cache: false,
                success: function (result) {
                    if (result == 1) {
                        //$("#maildiv").height(25);
                        $("#VerificationDiv").hide();
                        $("#verify-checkmark").show();
                        $("#OTPInvalid").hide();
                        $("#OTPExpire").hide();
                        $("#OTPBlank").hide();
                        document.getElementById("txtVerifyEmailOTP").value = "";
                        $("#txtEmail").prop("disabled", true).addClass("k-state-disabled");


                    }
                    else if (result == 2) {
                        $("#OTPBlank").hide();
                        $("#OTPExpire").hide();
                        $("#OTPInvalid").show();
                    }
                    else if (result == 3) {
                        $("#OTPBlank").hide();
                        $("#OTPInvalid").hide();
                        $("#OTPExpire").show();

                        //Disable submit
                        SubmitDisableEmail();
                        //Enable resend
                        ResendEnableEmail();
                    }
                    else {
                        alert("Something wrong on OTP");
                    }
                },
                error: function (result) {
                }
            })
        }

        //-----OTP ReSending functionality for Email
        function btnResendMailOTP_Click() {

            document.getElementById("txtVerifyEmailOTP").value = "";
            $("#OTPBlank").hide();
            $("#OTPExpire").hide();
            $("#OTPInvalid").hide();


            //Disable Resend
            ResendDisableEmail();

            // Again otp is sending to that email
            $.ajax({
                type: "POST",
                url: "/UserSignup/Signup/MailSend",
                data: { toEmail: $('#txtEmail').val() },
                datatype: "json",
                cache: false,
                success: function (result) {
                },
                error: function (result) {
                }
            })
            ///End Ajax call

            //Enable submit
            SubmitEnableEmail();

            EmailsubmitResendActiveOperation();

        }


    </script>
    @*Mail Verification css and Script function End*@

