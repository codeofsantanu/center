            function openchangefeeddiv() {
                debugger;
                var divv1 = document.getElementById('FeedBacklanding');
                divv1.style.display = "block";
                $("#ember1033").hide();

                $("#txtfeedback").val("");
                $("#username").val("");
                $("#txttEmail").val("");
                $("#RegistrationCode").val("");

                $.ajax({
                    type: "GET",
                    url: "/ClientAdmin/ClientDashBoard/GetUserPhotoByUserRegId",
                    datatype: "json",
                    cache: false,
                    success: function (data) {
                        debugger

                        document.getElementById("username").innerHTML = data[0].User_DisplayName;
                        document.getElementById("txttEmail").value = data[0].User_EmailId;
                        //  document.getElementById("RegistrationnCode").innerHTML = data[0].User_Unique_RegistrationCode;
                        $("#proimgg").attr("src", data[0].Uplaod_ImageURL);

                    }



                });
            }


