    <script>
        //if (document.getElementById("sid_nav_inner").style.visibility == "visible") {
        //    document.getElementById("sid_nav_inner").setAttribute("style", "transition: visibility 9s ease-in-out;");
        //}
        function sidenavminimize() {
            document.getElementById("side_nav").style.width = "0px";
            document.getElementById("mainbody").setAttribute("style", "margin-left: 30px; width: 97%;");
            document.getElementById("sid_nav_inner").style.visibility = "hidden";
            document.getElementById("icon_nav").style.display = "block";
}

        function sidenavmaximize() {
            document.getElementById("side_nav").style.width = "200px";
            document.getElementById("mainbody").setAttribute("style","margin-left: 200px;width: 85%;");
            document.getElementById("sid_nav_inner").style.visibility = "visible";
            document.getElementById("icon_nav").style.display = "none";
        }
        
    </script>
    <style>
#side_nav{
    transition: width .3s ease-in-out;
    
}
