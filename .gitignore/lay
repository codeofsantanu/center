        <script>
            var numStars;
            $(function () {
                $("div.star-rating > s, div.star-rating-rtl > s").on("click", function (e) {

                    // remove all active classes first, needed if user clicks multiple times
                    $(this).closest('div').find('.active').removeClass('active');

                    $(e.target).parentsUntil("div").addClass('active'); // all elements up from the clicked one excluding self
                    $(e.target).addClass('active');  // the element user has clicked on


                    numStars = $(e.target).parentsUntil("div").length + 1;
                    $('.show-result').text(numStars + (numStars == 1 ? " star" : " stars!"));
                });
            });

            function savebtnn() {

                var validator = $("#FeedBacklanding").kendoValidator().data("kendoValidator");

                if (validator.validate()) {
                    debugger;


                    $.ajax({
                        type: "POST",
                        url: "/ClientAdmin/ClientDashBoard/Savefeedback",
                        data: {
                            Rating: numStars,
                            Text: $("#txtfeedback").val(),
                        },
                        datatype: "json",
                        cache: false,
                        success: function (result) {
                            debugger;
                            var reset = document.getElementById("FeedBacklanding");
                            reset.style.display = "none";
                        },
                        error: function (result) {
                        }
                    })
                }
            }

        </script>
