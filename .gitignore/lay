    @* akan change password srt*@
    <script type="text/javascript">
        $(document).ready(function () {
            ////debugger;
            $("#chk1").hide();
            $("#chk2").hide();
            $("#chk3").hide();
            $("#chk4").hide();
            $("#imgverify").hide();
            $("#chk5").hide();
            //  $("#Next").kendoButton({}).data("kendoButton").enable(false);
            $('#txtconfirmpassword').change(function () {
                //debugger;
                if ($('#txtconfirmpassword').val() == "") {

                    document.getElementById('errormsg').innerText = '';
                }
                else if ($('#txtconfirmpassword').val() != $('#txtpassword').val()) {

                    document.getElementById('errormsg').innerText = 'Password Not Matched';

                    $("#imgverify").hide();
                    alert("Password Not Matched");
                }
                else {

                    document.getElementById('errormsg').innerText = '';
                }
            })

            $('#Next').click(function () {
                //debugger;
                $("#divLoader").show();
                var y = $('#txtpassword').val();
                var z = $('#txtconfirmpassword').val();


                if (z == '' & y == '') {
                    //debugger;
                    alert("Please set a Password");

                    var popupNotification = $("#popupNotification").data("kendoNotification");
                    popupNotification.show({
                        title: "Error !",
                        message: "Please set a Password."
                    }, "error");
                    $("#divLoader").hide();
                } else {
                    if (y == z) {
                        //debugger;
                        if (checkpassword()) {
                            $.ajax({
                                type: "POST",
                                url: "/ClientAdmin/Userlist/index",
                                data: { 'password': $('#txtpassword').val() },
                                datatype: "json",
                                cache: false,
                                success: function (data) {
                                    if (data == "0") {
                                        //debugger;
                                        $("#divLoader").hide();
                                        document.getElementById('errormsg').innerText = 'Not Changed';

                                    }
                                    else {
                                        alert("Password Successfully Changed")
                                        // $('#chk1').show();
                                        //debugger;
                                        $('#errormsg').val('');
                                        $("#divLoader").hide();
                                        $("#imgverify").show();
                                        var popupNotification = $("#popupNotification").data("kendoNotification");
                                        popupNotification.show({
                                            title: "Success",
                                            message: "Password Successfully Changed."
                                        }, "upload-success");


                                    }
                                },
                            })
                        } else {


                            var popupNotification = $("#popupNotification").data("kendoNotification");
                            popupNotification.show({
                                title: "Error !",
                                message: "Password criteria not matched."
                            }, "error");
                            alert("Password criteria not matched");
                            $("#divLoader").hide();

                        }
                    }
                    else {
                        //debugger;
                        alert("New & Confirm Password are Not Matched");


                        var popupNotification = $("#popupNotification").data("kendoNotification");
                        popupNotification.show({
                            title: "Error !",
                            message: "New & Confirm Password are Not Matched."
                        }, "error");

                        $("#imgverify").hide();
                        $("#divLoader").hide();
                        $('#txtpassword').val('');
                        $('#txtconfirmpassword').val('');
                        $("#chk1").hide();
                        $("#chk2").hide();
                        $("#chk3").hide();
                        $("#chk4").hide();
                        $("#chk5").hide();

                    }
                }

            })
        })
        function checkpassword() {
            var pass = $('#txtpassword').val();
            var upper = 0;
            var lower = 0;
            var sym = 0;
            var num = 0;

            for (var i = 0; i < pass.length; i++) {
                var va = pass.charCodeAt(i);
                if (64 < va && va < 91) {
                    upper++;
                }
                else if (96 < va && va < 123) {
                    lower++;
                }
                else if (47 < va && va < 58) {
                    num++;
                }
                else if (34 < va && va < 39 || va == 64) {
                    sym++;
                }
            }
            if (pass.length > 7 && upper > 0 && lower > 0 && sym > 0) {
                return true;
            }
            else {
                return false;
            }
        }

        function passwordonly() {

            var pass = $('#txtpassword').val();
            var upper = 0;
            var lower = 0;
            var sym = 0;
            var num = 0;

            for (var i = 0; i < pass.length; i++) {
                var va = pass.charCodeAt(i);
                if (64 < va && va < 91) {
                    upper++;
                }
                else if (96 < va && va < 123) {
                    lower++;
                }
                else if (47 < va && va < 58) {
                    num++;
                }
                else if (34 < va && va < 39 || va == 64) {
                    sym++;
                }
            }
            if (pass.length > 7) {
                $("#chk1").show();
            }
            else {
                $("#chk1").hide();
            }
            if (upper > 0) {
                $("#chk2").show();
            }
            else {
                $("#chk2").hide();
            }
            if (lower > 0) {
                $("#chk3").show();
            }
            else {
                $("#chk3").hide();
            }
            if (num > 0) {
                $("#chk4").show();
            }
            else {
                $("#chk4").hide();
            }
            if (sym > 0) {
                $("#chk5").show();
            }
            else {
                $("#chk5").hide();
            }

        };

        function onchangeevent() {

        }




    </script>
